{
    "name": "laravel-hyperf/dispatcher",
    "description": "The dispatcher package for Laravel Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "dispatcher",
        "middleware",
        "swoole",
        "laravel-hyperf"
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "hyperf/context": "~3.1.0",
        "hyperf/dispatcher": "~3.1.0",
        "hyperf/pipeline": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelHyperf\\Dispatcher\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "LaravelHyperf\\Dispatcher\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
